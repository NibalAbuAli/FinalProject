{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "off",
      {
        "ignore": [
          "@page",
          "@page"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "apple-touch-icons": "off",
    "meta-viewport": "off",
    "manifest-exists": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not ios_saf <= 17",
    "not safari <= 17"
  ]
}